<!--create html for the "homepage" aka how community feed will appear-->

<ul class="post-list">
    {{#each posts as |post|}}
    <li class="post-item">
        {{> post-info post }}
    </li>
    {{/each}}
    <script src="/javascript/upvote.js"></script>
    <script src="/javascript/downvote.js"></script>
</ul>